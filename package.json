{
  "name": "based-ayu",
  "displayName": "Based Ayu",
  "description": "Based on vscode-ayu version 1.1.0",
  "version": "1.1.0",
  "publisher": "asyrjasalo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/asyrjasalo/vscode-ayu"
  },
  "bugs": {
    "url": "https://github.com/asyrjasalo/vscode-ayu/issues",
    "email": "opensource@syrjasalo.com"
  },
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "icon-theme",
    "theme",
    "based-ayu"
  ],
  "homepage": "https://github.com/asyrjasalo/vscode-ayu",
  "icon": "assets/logo.png",
  "contributes": {
    "themes": [
      {
        "label": "Based Ayu Dark",
        "uiTheme": "vs-dark",
        "path": "./based-ayu-dark.json"
      },
      {
        "label": "Based Ayu Dark Bordered",
        "uiTheme": "vs-dark",
        "path": "./based-ayu-dark-bordered.json"
      },
      {
        "label": "Based Ayu Light",
        "uiTheme": "vs",
        "path": "./based-ayu-light.json"
      },
      {
        "label": "Based Ayu Light Bordered",
        "uiTheme": "vs",
        "path": "./based-ayu-light-bordered.json"
      },
      {
        "label": "Based Ayu Mirage",
        "uiTheme": "vs-dark",
        "path": "./based-ayu-mirage.json"
      },
      {
        "label": "Based Ayu Mirage Bordered",
        "uiTheme": "vs-dark",
        "path": "./based-ayu-mirage-bordered.json"
      }
    ],
    "iconThemes": [
      {
        "id": "based-ayu",
        "label": "Based Ayu",
        "path": "./based-ayu-icons.json"
      }
    ]
  },
  "scripts": {
    "build": "ts-node ./src/build.ts",
    "package": "vsce package",
    "publish": "npm run build; vsce package; vsce publish"
  },
  "devDependencies": {
    "@types/node": "^16.6.0",
    "ayu": "^8.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3",
    "vsce": "^1.96.1"
  }
}
